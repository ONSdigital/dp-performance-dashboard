        <div class="col">
        {{#if trafficSources}}
            <div class="col col--lg-30">
                <h2>Top referrers</h2>
                <p class="flush">Sources that referred traffic</p>
                <div class="padding-right-lg--2 padding-left-sm--1 padding-left-md--1 margin-top--2 margin-right--1 margin-left-sm--0 margin-left-md--0 margin-left-lg--0 border--left-thick">
                    {{!-- <p class="margin-top--0">{{trafficSources.name}}</p> --}}
                    <ol>
                        {{#each trafficSources.values}}
                            <li class="flush">{{name}} <span style="float:right">{{sessions}}</span></li>
                        {{/each}}
                    </ol>
                </div>
            </div>
        {{/if}}

        {{#if landingPages}}
            <div class="col col--lg-29">
                <h2>Top landing pages</h2>
                <p class="flush">First page visited on users journey</p>
                <div class="padding-left-sm--1 padding-left-md--1 margin-top--2 margin-right--1 margin-left-sm--0 margin-left-md--0 margin-left-lg--0 border--left-thick">
                    {{!-- <p class="margin-top--0">{{landingPages.name}}</p> --}}
                    <ol>
                        {{#each landingPages.values}}
                            <li class="flush"><a href="https://www.ons.gov.uk{{uri}}" target="_blank" rel="noopener noreferrer">{{name}}</a> <span style="float:right">{{sessions}}</span></li>
                        {{/each}}
                    </ol>
                </div>
            </div>
        {{/if}}
    </div>

