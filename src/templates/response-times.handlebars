<div class="col-wrap border-bottom--iron-light-lg">
    <div class="col padding-bottom-sm--1 padding-bottom-md--6">
        <h3 class="margin-top--4">Website response times</h3>
        <p class="flush">Time taken to return a page request, measured through Pingdom</p>
        {{#each averageResponseTimes}}
            <div class="col col--lg-18 padding-right--1 padding-left-sm--1 padding-left-md--1 margin-top--2 margin-right--1 margin-left-sm--0 margin-left-md--0 margin-left-lg--0 border--left-thick">
                <p class="margin-top--0">{{description}}</p>
                <p class="size--24 flush">{{responseTime}}ms</p>
                <p class="flush">Uptime: {{percentageTimeUp}}%<br/> Downtime: {{percentageTimeDown}}%</p>
            </div>
        {{/each}}
    </div>
</div>