{
  "name": "performance",
  "version": "0.0.1",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "webpack-server": "ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist/ --port 9876",
    "webpack-production": "ENV=production node ./node_modules/webpack/bin/webpack.js",
    "install-sixteens": "mkdir -p node_modules && rm -rf ./node_modules/sixteens && cd node_modules && git clone -b live https://github.com/onsdigital/sixteens && cd sixteens && npm install && cp -rf dist ../../dist && cd ../ && rm -rf sixteens",
    "install": "npm run install-sixteens && npm run webpack-production"
  },
  "dependencies": {
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "highcharts": "^4.2.6",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "redux": "^3.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "xhr": "^2.2.2"
  }
}
